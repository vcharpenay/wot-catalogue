        skos:related     xsd:integer , lwm2m:RW .

<urn:oma:lwm2m:oma:res:5513>
        a                skos:Concept ;
        rdfs:label       "Max Z Value" ;
        skos:definition  "The maximum measured value along the Z axis" ;
        skos:related     xsd:float , lwm2m:R .

<urn:oma:lwm2m:ext:3331>
        a                skos:Concept ;
        rdfs:label       "Energy" ;
        skos:definition  "This IPSO object should be used to report energy consumption (Cumulative Power) of an electrical load. An example measurement unit is Watt Hours (ucum:W*h).\n        " ;
        skos:related     <urn:oma:lwm2m:oma:res:5822> , <urn:oma:lwm2m:oma:res:5805> , <urn:oma:lwm2m:oma:res:5750> , <urn:oma:lwm2m:oma:res:5701> .

