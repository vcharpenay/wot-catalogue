<urn:oma:lwm2m:oma:res:5550>
        a                skos:Concept ;
        rdfs:label       "Digital Output State" ;
        skos:definition  "The current state of a digital output." ;
        skos:related     xsd:boolean , lwm2m:RW .

<urn:oma:lwm2m:oma:res:5814>
        a                skos:Concept ;
        rdfs:label       "Max Range reactive power" ;
        skos:definition  "The maximum reactive power that can be measured by the sensor" ;
        skos:related     xsd:float , lwm2m:R .

<urn:oma:lwm2m:oma:res:5522>
        a                skos:Concept ;
        rdfs:label       "Clip" ;
        skos:definition  "Audio Clip that is playable (i.e. short audio recording indicating the floor in an elevator)." ;
        skos:related     xsd:hexBinary , lwm2m:RW .

<urn:oma:lwm2m:oma:res:5701>
        a                skos:Concept ;
        rdfs:label       "Sensor Units" ;
        skos:definition  "Measurement Units Definition e.g. “Cel” for Temperature in Celsius." ;
        skos:related     xsd:string , lwm2m:R .

<urn:oma:lwm2m:oma:res:5827>
        a                skos:Concept ;
        rdfs:label       "Avg Load Adj Pct" ;
        skos:definition  "Defines the maximum energy usage of the receivng device, as a percentage of the device's normal maximum energy usage." ;
        skos:related     xsd:string , lwm2m:RW .

<urn:oma:lwm2m:ext:3340>
        a                skos:Concept ;
        rdfs:label       "Timer" ;
        skos:definition  "This IPSO object is used to time events and actions, using patterns common to industrial timers. A POST to the trigger resource or On/Off input state change starts the timing operation, and the timer remaining time shows zero when the operation is complete. The patterns supported are One-Shot (mode 1), On-Time or Interval (mode 2), Time delay on pick-up or TDPU (tmode 3), and Time Delay on Drop-Out or TDDO (mode 4). Mode 0 disables the timer, so the output follows the input with no delay. A counter is provided to count occurrences of the timer output changing from 0 to 1. Writing a value of zero resets the counter. The Digital Input State resource reports the state of the timer output.\n        " ;
        skos:related     <urn:oma:lwm2m:oma:res:5544> , <urn:oma:lwm2m:oma:res:5521> , <urn:oma:lwm2m:oma:res:5543> , <urn:oma:lwm2m:oma:res:5534> , <urn:oma:lwm2m:oma:res:5526> , <urn:oma:lwm2m:oma:res:5525> , <urn:oma:lwm2m:oma:res:5501> , <urn:oma:lwm2m:oma:res:5850> , <urn:oma:lwm2m:oma:res:5538> , <urn:oma:lwm2m:oma:res:5523> , <urn:oma:lwm2m:oma:res:5750> .

<urn:oma:lwm2m:ext:3312>
        a                skos:Concept ;
        rdfs:label       "Power Control" ;
        skos:definition  "Description: This Object is used to control a power source, such as a Smart Plug.  It allows a power relay to be turned on or off and its dimmer setting to be control as a % between 0 and 100." ;
        skos:related     <urn:oma:lwm2m:oma:res:5820> , <urn:oma:lwm2m:oma:res:5851> , <urn:oma:lwm2m:oma:res:5850> , <urn:oma:lwm2m:oma:res:5852> , <urn:oma:lwm2m:oma:res:5805> , <urn:oma:lwm2m:oma:res:5750> .

<urn:oma:lwm2m:oma:res:5507>
        a                skos:Concept ;
        rdfs:label       "Fractional Time" ;
        skos:definition  "For shorter times of a fraction of a second (i.e. 0.23)." ;
        skos:related     xsd:float , lwm2m:RW .

<urn:oma:lwm2m:oma:res:5548>
        a                skos:Concept ;
        rdfs:label       "Level" ;
        skos:definition  "Audio volume control, float value between 0 and 100 as a percentage." ;
        skos:related     xsd:float , lwm2m:RW .

<urn:oma:lwm2m:ext:3325>
        a                skos:Concept ;
        rdfs:label       "Concentration" ;
        skos:definition  "This IPSO object should be used to the particle concentration measurement of a medium. It also provides resources for minimum and maximum measured values, as well as the minimum and maximum range that can be measured by the sensor. An example measurement unit is parts per million  (ucum: ppm).\n        " ;
        skos:related     <urn:oma:lwm2m:oma:res:5605> , <urn:oma:lwm2m:oma:res:5700> , <urn:oma:lwm2m:oma:res:5821> , <urn:oma:lwm2m:oma:res:5701> , <urn:oma:lwm2m:oma:res:5601> , <urn:oma:lwm2m:oma:res:5602> , <urn:oma:lwm2m:oma:res:5750> , <urn:oma:lwm2m:oma:res:5604> , <urn:oma:lwm2m:oma:res:5603> .

<urn:oma:lwm2m:oma:res:5601>
        a                skos:Concept ;
        rdfs:label       "Min Measured Value" ;
        skos:definition  "The minimum value measured by the sensor since power ON or reset" ;
        skos:related     xsd:float , lwm2m:R .

<urn:oma:lwm2m:ext:3338>
        a                skos:Concept ;
        rdfs:label       "Buzzer" ;
        skos:definition  "This IPSO object should be used to actuate an audible alarm such as a buzzer, beeper, or vibration alarm. There is a dimmer control for setting the relative loudness of the alarm, and an optional duration control to limit the length of time the alarm sounds when turned on. Each time a “1” is written to the On/Off resource, the alarm will sound again for the configured duration. If no duration is programmed or the setting is zero, writing a “1” to the On/Off resource will result in the alarm sounding continuously until a “0” is written to the On/Off resource.\n        " ;
        skos:related     <urn:oma:lwm2m:oma:res:5850> , <urn:oma:lwm2m:oma:res:5525> , <urn:oma:lwm2m:oma:res:5521> , <urn:oma:lwm2m:oma:res:5750> , <urn:oma:lwm2m:oma:res:5548> .

<urn:oma:lwm2m:oma:res:5821>
        a                skos:Concept ;
        rdfs:label       "Current Calibration" ;
        skos:definition  "Read or Write the current calibration coefficient" ;
        skos:related     xsd:float , lwm2m:RW .

<urn:oma:lwm2m:oma:res:5501>
        a                skos:Concept ;
        rdfs:label       "Digital Input Counter" ;
        skos:definition  "The cumulative value of active state detected." ;
        skos:related     xsd:integer , lwm2m:R .

<urn:oma:lwm2m:oma:res:5806>
        a                skos:Concept ;
        rdfs:label       "Active Power Calibration" ;
        skos:definition  "Request an active power calibration by writing the value of a calibrated load." ;
        skos:related     xsd:float , lwm2m:W .

<urn:oma:lwm2m:oma:res:5542>
        a                skos:Concept ;
        rdfs:label       "Down Counter" ;
        skos:definition  "Counts the times the decrease control has been operated. Writing a 0 resets the counter." ;
        skos:related     xsd:integer , lwm2m:RW .

<urn:oma:lwm2m:oma:res:5514>
        a                skos:Concept ;
        rdfs:label       "Longitude" ;
        skos:definition  "The decimal notation of longitude, e.g. 153.21760 (World Geodetic System 1984)." ;
        skos:related     xsd:string , lwm2m:R .

<urn:oma:lwm2m:oma:res:5900>
        a                skos:Concept ;
        rdfs:label       "Set Point Value" ;
        skos:definition  "The setpoint value." ;
        skos:related     xsd:float , lwm2m:RW .

<urn:oma:lwm2m:ext:3332>
        a                skos:Concept ;
        rdfs:label       "Direction" ;
        skos:definition  "This IPSO object is used to report the direction indicated by a compass, wind vane, or other directional indicator. The units of measure is plane angle degrees (ucum:deg).\n        " ;
        skos:related     <urn:oma:lwm2m:oma:res:5705> , <urn:oma:lwm2m:oma:res:5602> , <urn:oma:lwm2m:oma:res:5601> , <urn:oma:lwm2m:oma:res:5750> , <urn:oma:lwm2m:oma:res:5605> .

<urn:oma:lwm2m:oma:res:5527>
        a                skos:Concept ;
        rdfs:label       "Text" ;
        skos:definition  "A string of text." ;
        skos:related     xsd:string , lwm2m:RW .

<urn:oma:lwm2m:ext:3304>
        a                skos:Concept ;
        rdfs:label       "Humidity" ;
        skos:definition  "Description: This IPSO object should be used with a humidity sensor to report a humidity measurement.  It also provides resources for minimum/maximum measured values and the minimum/maximum range that can be measured by the humidity sensor. An example measurement unit is relative humidity as a percentage (ucum:%)." ;
        skos:related     <urn:oma:lwm2m:oma:res:5603> , <urn:oma:lwm2m:oma:res:5601> , <urn:oma:lwm2m:oma:res:5701> , <urn:oma:lwm2m:oma:res:5605> , <urn:oma:lwm2m:oma:res:5602> , <urn:oma:lwm2m:oma:res:5604> , <urn:oma:lwm2m:oma:res:5700> .

<urn:oma:lwm2m:oma:res:5706>
        a                skos:Concept ;
        rdfs:label       "Colour" ;
        skos:definition  "A string representing a value in some color space" ;
        skos:related     xsd:string , lwm2m:RW .

<urn:oma:lwm2m:ext:3345>
        a                skos:Concept ;
        rdfs:label       "Multiple Axis Joystick" ;
        skos:definition  "This IPSO object can be used to report the position of a shuttle or joystick control. A digital input is provided to report the state of an associated push button.\n        " ;
        skos:related     <urn:oma:lwm2m:oma:res:5501> , <urn:oma:lwm2m:oma:res:5704> , <urn:oma:lwm2m:oma:res:5703> , <urn:oma:lwm2m:oma:res:5750> , <urn:oma:lwm2m:oma:res:5702> , <urn:oma:lwm2m:oma:res:5500> .

<urn:oma:lwm2m:ext:3317>
        a                skos:Concept ;
        rdfs:label       "Current" ;
        skos:definition  "This IPSO object should be used with an ammeter to report measured electric current in amperes. It also provides resources for minimum and maximum measured values, as well as the minimum and maximum range that can be measured by the sensor. An example measurement unit is volts (ucum: A).\n        " ;
        skos:related     <urn:oma:lwm2m:oma:res:5601> , <urn:oma:lwm2m:oma:res:5604> , <urn:oma:lwm2m:oma:res:5750> , <urn:oma:lwm2m:oma:res:5603> , <urn:oma:lwm2m:oma:res:5701> , <urn:oma:lwm2m:oma:res:5602> , <urn:oma:lwm2m:oma:res:5605> , <urn:oma:lwm2m:oma:res:5700> , <urn:oma:lwm2m:oma:res:5821> .

<urn:oma:lwm2m:oma:res:5800>
