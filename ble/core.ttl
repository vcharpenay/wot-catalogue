@prefix ble: <http://purl.org/wot-catalogue/ble#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://purl.org/wot-catalogue/ble/core#> a skos:Collection;
  skos:altLabel "ble metamodel";
  skos:member ble:Characteristic, ble:Profile, ble:Property, ble:Scheme, ble:Service,
    ble:Value;
  skos:prefLabel "BLE Metamodel" .

<http://purl.org/wot-catalogue/ble/core#Characteristic> a skos:Concept;
  skos:altLabel "characteristic";
  skos:inScheme ble:Scheme;
  skos:narrower ble:AlertLevel, ble:BatteryLevel, ble:BondManagementControlPoint, ble:BondManagementFeature,
    ble:CurrentTime, ble:DatabaseChangeIncrement, ble:FirmwareRevision, ble:HardwareRevision,
    ble:IEEE11073-20601RegulatoryCertificationDataList, ble:LocalTimeInformation, ble:ManufacturerName,
    ble:ModelNumber, ble:PnPID, ble:RecordAccessControlPoint, ble:ReferenceTimeInformation,
    ble:SCControlPoint, ble:ScanIntervalWindow, ble:ScanRefresh, ble:SensorLocation, ble:SerialNumber,
    ble:SoftwareRevision, ble:SystemID, ble:UDSCharacteristic, ble:UserControlPoint, ble:UserIndex;
  skos:prefLabel "Characteristic";
  skos:related ble:Property, ble:Value .

<http://purl.org/wot-catalogue/ble/core#Scheme> a skos:ConceptScheme;
  rdfs:comment "Generic Attribute Profile (GATT) is built on top of the Attribute Protocol (ATT) and establishes common operations and a framework for the data transported and stored by the Attribute Protocol. For more information on GATT, please refer to Volume 0, Section 6 of the Bluetooth Core Specification.";
  rdfs:seeAlso <https://www.bluetooth.com/specifications/gatt/>;
  skos:altLabel "bluetooth low energy - general attribute profile";
  skos:hasTopConcept ble:Profile;
  skos:prefLabel "Bluetooth Low Energy - General Attribute Profile" .

<http://purl.org/wot-catalogue/ble/core#Value> a skos:Concept;
  skos:altLabel "value";
  skos:inScheme ble:Scheme;
  skos:narrower ble:BluetoothSIG, ble:CumulativeCrankRevolutions, ble:DatabaseChangeIncrementValue,
    ble:InstantaneousPowerField, ble:InstantaneousSpeedField, ble:LE_Scan_Interval, ble:LE_Scan_Window,
    ble:LastCrankEventTime, ble:ManufacturerIdentifier, ble:OrganizationallyUniqueIdentifier,
    ble:ProductIDField, ble:ProductVersionField, ble:Serverrequiresrefresh, ble:TimeStampField,
    ble:UDSCharacteristicValue, ble:USBImplementerForum, ble:UserIDField, ble:VendorIDField,
    ble:VendorIDSourceField;
  skos:prefLabel "Value" .

<http://purl.org/wot-catalogue/ble/core#Service> a skos:Concept;
  skos:altLabel "service";
  skos:inScheme ble:Scheme;
  skos:narrower ble:BatteryService, ble:BondManagementService, ble:CurrentTimeService,
    ble:DeviceInformationService, ble:ImmediateAlertService, ble:ScanParametersService,
    ble:UserDataService;
  skos:prefLabel "Service";
  skos:related ble:Characteristic .

<http://purl.org/wot-catalogue/ble/core#Profile> a skos:Concept;
  skos:altLabel "profile";
  skos:inScheme ble:Scheme;
  skos:prefLabel "Profile";
  skos:related ble:Service .

<http://purl.org/wot-catalogue/ble/core#Property> a skos:Concept;
  skos:altLabel "property";
  skos:inScheme ble:Scheme;
  skos:narrower ble:Indicate, ble:Notify, ble:Read, ble:ReadLong, ble:ReliableWrite,
    ble:Write, ble:WritewithoutResponse;
  skos:prefLabel "Property" .
