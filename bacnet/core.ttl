@prefix bacnet: <http://purl.org/wot-catalogue/bacnet#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://purl.org/wot-catalogue/bacnet/core#> a skos:Collection;
  skos:altLabel "bacnet metamodel";
  skos:member bacnet:OptionalProperty, bacnet:ReadableProperty, bacnet:Scheme, bacnet:StandardObject,
    bacnet:WritableProperty, bacnet:servicesSupported;
  skos:prefLabel "BACnet Metamodel" .

<http://purl.org/wot-catalogue/bacnet/core#StandardObject> a skos:Concept;
  skos:altLabel "standard object";
  skos:inScheme bacnet:Scheme;
  skos:narrower bacnet:OptionalProperty, bacnet:ReadableProperty, bacnet:WritableProperty;
  skos:prefLabel "StandardObject" .

<http://purl.org/wot-catalogue/bacnet/core#OptionalProperty> a skos:Concept;
  skos:altLabel "optional property";
  skos:inScheme bacnet:Scheme;
  skos:prefLabel "OptionalProperty" .

<http://purl.org/wot-catalogue/bacnet/core#Scheme> a skos:ConceptScheme;
  rdfs:seeAlso <http://bacowl.sourceforge.net/>;
  skos:altLabel "bacnet";
  skos:hasTopConcept bacnet:servicesSupported;
  skos:prefLabel "BACnet" .

<http://purl.org/wot-catalogue/bacnet/core#WritableProperty> a skos:Concept;
  skos:altLabel "writable property";
  skos:inScheme bacnet:Scheme;
  skos:prefLabel "WritableProperty" .

<http://purl.org/wot-catalogue/bacnet/core#ReadableProperty> a skos:Concept;
  skos:altLabel "readable property";
  skos:inScheme bacnet:Scheme;
  skos:prefLabel "ReadableProperty" .

<http://purl.org/wot-catalogue/bacnet/core#servicesSupported> a skos:Concept;
  skos:altLabel "services supported";
  skos:inScheme bacnet:Scheme;
  skos:narrower bacnet:StandardObject;
  skos:prefLabel "servicesSupported" .
